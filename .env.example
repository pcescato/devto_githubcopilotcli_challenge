# Example .env file for DEV.to Analytics
# Copy this to .env and update with your values

# DEV.to API Configuration
DEVTO_API_KEY=your_devto_api_key_here

# PostgreSQL Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=devto
POSTGRES_PASSWORD=change_this_secure_password
POSTGRES_DB=devto_analytics

# PostgreSQL Performance Tuning (adjust based on your server)
POSTGRES_SHARED_BUFFERS=256MB
POSTGRES_EFFECTIVE_CACHE_SIZE=1GB
POSTGRES_MAINTENANCE_WORK_MEM=128MB
POSTGRES_WORK_MEM=16MB
POSTGRES_MAX_CONNECTIONS=100

# Valkey (Redis alternative) Configuration
VALKEY_HOST=localhost
VALKEY_PORT=6379
VALKEY_PASSWORD=change_this_secure_password

# DbGate Configuration
DBGATE_PORT=3000

# Apache Superset Configuration
SUPERSET_PORT=8088
SUPERSET_SECRET_KEY=CHANGE_THIS_TO_A_LONG_RANDOM_SECRET_KEY_MINIMUM_32_CHARS
SUPERSET_ADMIN_USERNAME=admin
SUPERSET_ADMIN_PASSWORD=change_this_admin_password
SUPERSET_ADMIN_EMAIL=admin@devto-analytics.local
SUPERSET_ADMIN_FIRSTNAME=Admin
SUPERSET_ADMIN_LASTNAME=User

# FastAPI Application (planned)
FASTAPI_PORT=8000
FASTAPI_HOST=0.0.0.0
FASTAPI_WORKERS=4
FASTAPI_RELOAD=false

# Environment
ENVIRONMENT=development
DEBUG=false
LOG_LEVEL=INFO
